{
  "name": "@naivemap/maplibre-gl-echarts-layer",
  "version": "0.0.1-alpha.1",
  "description": "A MapLibre GL JS layer to integrate Apache ECharts' Lines and Scatter charts.",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "maplibre",
    "echarts",
    "layer",
    "custom"
  ],
  "author": "naivemap",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/naivemap/maplibre-gl-layers",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "typedocs": "pnpm typedoc --watch"
  },
  "peerDependencies": {
    "echarts": "^5.0.0",
    "maplibre-gl": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "echarts": "^5.6.0",
    "maplibre-gl": "^5.6.1"
  }
