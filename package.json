{
  "name": "@naivemap/maplibre-gl-echarts-layer",
  "version": "0.0.0",
  "description": "A collection of custom layer implementations for MapLibre GL JS.",
  "private": true,
  "keywords": [
    "maplibre",
    "echarts",
    "image",
    "layer",
    "custom"
  ],
  "author": "naivemap",
  "license": "MIT",
  "repository": "https://github.com/naivemap/maplibre-gl-layers",
  "scripts": {
    "dev": "vite",
    "build": "pnpm -r build",
    "typedoc": "pnpm -r typedocs",
    "gen-docs-example": "node ./scripts/gen-docs-example.js",
    "docs:example": "pnpm gen-docs-example",
    "docs:dev": "pnpm build && concurrently \"pnpm typedoc\" \"vitepress dev docs\"",
    "docs:build": "pnpm build && pnpm typedoc && pnpm docs:example && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/node": "^22.14.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "echarts": "^5.6.0",
    "less": "^4.3.0",
    "maplibre-gl": "^5.6.1",
    "markdown-it-mathjax3": "^4.3.2",
    "prettier": "3.6.2",
    "shiki": "^3.7.0",
    "typedoc": "^0.28.6",
    "typedoc-plugin-markdown": "^4.7.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-bundle-obfuscator": "^1.7.0",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "2.0.0-alpha.7",
    "vitest": "^3.2.4"
  }
}
